<?xml version="1.0" encoding="utf-8"?>
<layouts>
	<!-- Scenario meta -->
	<layout for="scnr">
		<int32 name="script string table size" offset="0x3E0" />
		<uint32 name="script string table address" offset="0x3EC" />

		<int32 name="number of scripts" offset="0x3F4" />
		<uint32 name="script table address" offset="0x3F8" />

		<int32 name="number of script globals" offset="0x400" />
		<uint32 name="script global table address" offset="0x404" />

		<int32 name="number of script expressions" offset="0x4A4" />
		<uint32 name="script expression table address" offset="0x4A8" />

		<int32 name="number of script tag references" offset="0x40C" />
		<uint32 name="script tag references table address" offset="0x410" />
	</layout>

	<layout for="script element" inherits="script element" />

	<layout for="script parameter element" inherits="script parameter element" />

	<layout for="script global element" inherits="script global element" />

	<layout for="script expression element" inherits="script expression element" />
</layouts>