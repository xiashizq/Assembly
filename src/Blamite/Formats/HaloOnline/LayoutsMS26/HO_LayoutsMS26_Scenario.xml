<?xml version="1.0" encoding="utf-8"?>
<layouts>
	<!-- Scenario meta -->
	<layout for="scnr">
		<int32 name="number of script objects" offset="0xBC" />
		<uint32 name="script object table address" offset="0xC0" />

		<int32 name="script string table size" offset="0x410" />
		<uint32 name="script string table address" offset="0x41C" />

		<int32 name="number of scripts" offset="0x424" />
		<uint32 name="script table address" offset="0x428" />

		<int32 name="number of script globals" offset="0x430" />
		<uint32 name="script global table address" offset="0x434" />

		<int32 name="number of script expressions" offset="0x4D4" />
		<uint32 name="script expression table address" offset="0x4D8" />

		<int32 name="number of script tag references" offset="0x43C" />
		<uint32 name="script tag references table address" offset="0x440" />
	</layout>

	<layout for="script object element" inherits="script object element" />

	<layout for="script element" inherits="script element" />

	<layout for="script parameter element" inherits="script parameter element" />

	<layout for="script global element" inherits="script global element" />

	<layout for="script expression element" inherits="script expression element" />
</layouts>