<?xml version="1.0" encoding="utf-8"?>
<layouts>
	<layout for="header" inherits="header" />

	<layout for="meta header" size="0x28">
		<int32 name="tag table address" offset="0x0" />
		<int32 name="scenario datum index" offset="0x4" />
		<int32 name="checksum" offset="0x8" />
		<int32 name="number of tags" offset="0xC" />
		<int32 name="number of model parts" offset="0x10" />
		<int32 name="model data offset" offset="0x14" />
		<int32 name="number of model parts alt" offset="0x18" />
		<int32 name="vertices size" offset="0x1C" />
		<int32 name="model data size" offset="0x20" />
		<int32 name="magic" offset="0x24" />
	</layout>

	<layout for="tag element" inherits="tag element" />

	<layout for="bsp header" inherits="bsp header" />

	<layout for="tag block" inherits="tag block" />

	<layout for="tag reference" inherits="tag reference" />

	<layout for="data reference" inherits="data reference" />

	<!-- hmt (hud_message_text) -->
	<layout for="hmt" inherits="hmt" />

	<layout for="hud message element" inherits="hud message element" />

	<layout for="hud message" inherits="hud message" />

	<!-- str# (string_list) / ustr (unicode_string_list) -->
	<layout for="str" inherits="str" />

	<layout for="string list element" inherits="string list element" />
</layouts>
