{
  "version": "1.0.0",
  "title": "Blamite",
  "description": "DotNet library for interacting with Halo data.",
  "authors": [
    "Aaron Dierking",
    "TJ Tunnell",
    "Alex Forbes-Reed",
    "Lord Zedd"
  ],
  "packOptions": {
    "summary": "DotNet library for interacting with Halo data.",
    "tags": [
      "halo",
      "blam",
      "modding",
      "xbox"
    ],
    "repository": {
      "type": "git",
      "url": "https://github.com/XboxChaos/Assembly"
    }
  },
  "buildOptions": {
    "allowUnsafe": true,
    "debugType": "portable"
  },
  "dependencies": {
    "Antlr4.Runtime.Standard": "4.8.0",
    "Newtonsoft.Json": "13.0.2"
  },
  "frameworks": {
    "net48": {
      "buildOptions": {
        "define": [
          "NET48"
        ]
      },
      "frameworkAssemblies": {
        "System.Data": "",
        "System.Numerics": "",
        "System.Runtime.Serialization": "",
        "System.Xml": "",
        "System.Xml.Linq": ""
      }
    }
  },
  "runtimes": {
    "win-x86": {},
    "win-x64": {}
  }
}